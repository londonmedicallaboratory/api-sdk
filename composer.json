{
    "name": "londonmedicallaboratory/api-sdk",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "zmitic",
            "email": "mitke013@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/http-kernel": "^5.4",
        "symfony/http-client": "^5.4",
        "endroid/installer": "^1.3",
        "londonmedicallaboratory/view-bundle": "dev-master",
        "react/http": "^1.5",
        "clue/block-react": "^1.4",
        "brick/money": "^0.5.2",
        "symfony/twig-bundle": "^5.4",
        "omnipay/sagepay": "dev-feature/pi-protocol-4",
        "symfony/intl": "^5.4",
        "sensio/framework-extra-bundle": "^5.4",
        "twig/extra-bundle": "^2.12",
        "twig/string-extra": "^3.3",
        "symfony/framework-bundle": "^5.4",
        "strictify/form-mapper-bundle": "dev-master",
        "strictify/goodies-bundle": "dev-master",
        "symfony/form": "^5.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "dev-master",
        "psalm/plugin-symfony": "^3.0",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/yaml": "^5.4",
        "symfony/stopwatch": "^5.4",
        "bocmah/psalm-reactphp-promise-plugin": "^0.1.1",
        "symfony/web-profiler-bundle": "^5.4"
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/strictify/form-mapper-bundle"
        },
        {
            "type": "github",
            "url": "https://github.com/londonmedicallaboratory/view-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/benjam-es/omnipay-sagepay"
        },
        {
            "type": "github",
            "url": "https://github.com/strictify/goodies-bundle.git"
        }
    ],
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": {
            "LML\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LML\\SDK\\Tests\\": "tests",
            "LML\\SDK\\Fixture\\": "tests\\fixtures"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}

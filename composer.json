{
    "name": "londonmedicallaboratory/api-sdk",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "symfony-ux"
    ],
    "authors": [
        {
            "name": "zmitic",
            "email": "mitke013@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/http-kernel": "^6",
        "symfony/http-client": "^6",
        "endroid/installer": "^1.4",
        "londonmedicallaboratory/view-bundle": "dev-master",
        "react/http": "^1.7",
        "brick/money": "^0.5.3",
        "symfony/twig-bundle": "^6",
        "omnipay/sagepay": "dev-feature/pi-protocol-4",
        "symfony/intl": "^6",
        "sensio/framework-extra-bundle": "^6",
        "twig/extra-bundle": "^3.4",
        "twig/string-extra": "^3.4",
        "symfony/framework-bundle": "^6",
        "strictify/form-mapper-bundle": "dev-master",
        "strictify/goodies-bundle": "dev-master",
        "symfony/form": "^6",
        "webmozart/assert": "^1.11",
        "symfony/security-bundle": "^6",
        "nyholm/psr7": "^1.7",
        "php-http/httplug": "^2.4",
        "react/async": "^4",
        "symfony/lock": "^6.0|^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.18.0",
        "psalm/plugin-symfony": "dev-symfony-forms-6.3-compatibility",
        "symfony/phpunit-bridge": "^6",
        "symfony/yaml": "^6",
        "symfony/stopwatch": "^6",
        "bocmah/psalm-reactphp-promise-plugin": "dev-main",
        "symfony/web-profiler-bundle": "^6"
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/strictify/form-mapper-bundle"
        },
        {
            "type": "github",
            "url": "https://github.com/zmitic/psalm-reactphp-promise-plugin.git"
        },
        {
            "type": "github",
            "url": "https://github.com/londonmedicallaboratory/view-bundle.git"
        },
        {
            "type": "github",
            "url": "https://github.com/strictify/goodies-bundle.git"
        },
        {
            "type": "github",
            "url": "https://github.com/benjam-es/omnipay-sagepay"
        },
        {
            "type": "github",
            "url": "https://github.com/zmitic/psalm-plugin-symfony.git"
        }
    ],
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "LML\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LML\\SDK\\Tests\\": "tests",
            "LML\\SDK\\Fixture\\": "tests\\fixtures"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.0.*"
        }
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true
        }
    }
}

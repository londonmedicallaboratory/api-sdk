{
    "name": "londonmedicallaboratory/api-sdk",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "zmitic",
            "email": "mitke013@gmail.com"
        }
    ],
    "require": {
        "symfony/http-kernel": "^5.3",
        "symfony/http-client": "^5.3",
        "endroid/installer": "^1.3",
        "londonmedicallaboratory/view-bundle": "dev-master",
        "react/http": "^1.5",
        "clue/block-react": "^1.4",
        "brick/money": "^0.5.2",
        "symfony/twig-bundle": "^5.3",
        "omnipay/sagepay": "^3.2",
        "symfony/intl": "^5.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.9",
        "psalm/plugin-symfony": "^2.4",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/yaml": "^5.3",
        "symfony/stopwatch": "^5.3",
        "bocmah/psalm-reactphp-promise-plugin": "^0.1.1"
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/londonmedicallaboratory/view-bundle.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "LML\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LML\\SDK\\Tests\\": "tests",
            "LML\\SDK\\Fixture\\": "tests\\fixtures"
        }
    }
}
